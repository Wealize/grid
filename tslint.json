{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "semicolon": [true, "never"],
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-consecutive-blank-lines": [true, 2],
    "max-line-length": false,
    "interface-name": [true, "never-prefix"]
  },
  "rulesDirectory": []
}
